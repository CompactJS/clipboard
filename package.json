{
  "name": "@compactjs/clipboard",
  "version": "0.0.1",
  "description": "Copy to clipboard.",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "microbundle && npm run build:example",
    "build:example": "microbundle build -f es -o example/dist/index.mjs",
    "dev": "microbundle watch -f es -o example/dist/index.mjs",
    "prepare": "npm run build",
    "lint": "eslint src",
    "test": "npm run lint"
  },
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CompactJS/clipboard.git"
  },
  "keywords": [
    "javascript",
    "clipboard",
    "copy",
    "compactjs"
  ],
  "author": "Timo Bechtel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CompactJS/clipboard/issues"
  },
  "homepage": "https://github.com/CompactJS/clipboard#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "eslint": "^6.8.0",
    "microbundle": "^0.12.0-next.8",
    "semantic-release": "^17.0.4"
  }
}
